import ij.plugin.frame.RoiManager;
import ij.gui.Roi;
import ij.IJ;
import ij.ImagePlus;
import ij.plugin.RoiScaler;

String pathToRois = "/Volumes/Macintosh/curioData/test/movie_X0_Y0/rescaled_MMStack_Pos0.ome_ROI.zip";
//ImagePlus img = IJ.openImage("/Volumes/Macintosh/curioData/test/movie_X0_Y0/rescaled_MMStack_Pos0.ome_BinaryImage.tif");
ImagePlus img = IJ.getImage();
roiManager = new RoiManager();
roiManager.runCommand("Open", pathToRois);
Roi[] roiArray = roiManager.getRoisAsArray();
img.setRoi(roiArray[0]);
img.show();
img.setRoi(RoiScaler.scale(roiArray[0],
				1.7,
				1.44, false));
img.show();

roiManager.close();